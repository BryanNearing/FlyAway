		<description></description>
		<servlet-class>com.Servlets.AdminLogin</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ChangePassword</servlet-name>
		<display-name>ChangePassword</display-name>
		<description></description>
		<servlet-class>com.Servlets.ChangePassword</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Logout</servlet-name>
		<display-name>Logout</display-name>
		<description></description>
		<servlet-class>com.Servlets.Logout</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AddAirline</servlet-name>
		<display-name>AddAirline</display-name>
