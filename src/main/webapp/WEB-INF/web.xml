	<servlet>
		<servlet-name>TakeInBillingInfo</servlet-name>
		<display-name>TakeInBillingInfo</display-name>
		<description></description>
		<servlet-class>com.Servlets.TakeInBillingInfo</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ChangePassword</servlet-name>
		<url-pattern>/ChangePassword</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddAirline</servlet-name>
		<url-pattern>/AddAirline</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddPlace</servlet-name>
		<url-pattern>/AddPlace</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddFlight</servlet-name>
